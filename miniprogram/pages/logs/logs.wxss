.containeBox{
  width: 100%;
  height: 100%;
  position: relative;
}
.containeBox image{
  width: 100%;
  height: auto;
}
.dTime{
  position: absolute;
  z-index: 100;
  bottom: 20px;
  right: 20px;
  width: 100rpx;
  height: 40rpx;
  font-size: 16px;
  color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  background: rgba(0, 0,0,.2 );
  border-radius: 4px;
}